{
  "name": "gallant-gallery-server",
  "version": "1.0.0",
  "description": "Gallant Gallery Server - TypeScript backend with image gallery API",
  "main": "../dist/server.js",
  "scripts": {
    "start": "node ../dist/server.js",
    "dev": "npm run build && nodemon ../dist/server.js",
    "dev:ts": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "gallery",
    "images",
    "thumbnails",
    "express",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "path": "^0.12.7",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.13",
    "@types/sharp": "^0.32.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
